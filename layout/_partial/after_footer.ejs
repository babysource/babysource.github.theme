<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
<script src="<%- config.root %>js/jquery.imagesloaded.min.js"></script>
<script src="<%- config.root %>js/gallery.js"></script>

<% if (theme.comment_provider == "disqus" && (theme.comment_disqus || 0).short_name) { %>
<script type="text/javascript">
(function(){
  var doc = document, dsq = doc.createElement('script');
  dsq.type = 'text/javascript';
  dsq.async = true;
  dsq.src = '//<%= theme.comment_disqus.short_name %>.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
  (doc.getElementsByTagName('head')[0] || doc.getElementsByTagName('body')[0]).appendChild(dsq);
}());
</script>
<% } %>

<% if (theme.fancybox) { %>
<link rel="stylesheet" href="<%- config.root %>fancybox/jquery.fancybox.css" media="screen" type="text/css">
<script src="<%- config.root %>fancybox/jquery.fancybox.pack.js"></script>
<script type="text/javascript">
(function($){
  $('.fancybox').fancybox();
})(jQuery);
</script>
<% } %>